{
  "name": "lisa-prntr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{jsx,ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx} --config ./.prettierrc.json",
    "prepare": "husky install",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run format"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@vanilla-extract/vite-plugin": "^4.0.17",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.2",
    "prettier": "2.7.1",
    "vite": "^5.4.10"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/lisa-prntr.es.js",
      "require": "./dist/lisa-prntr.cjs.js"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "./dist/lisa-prntr.cjs.js",
  "module": "./dist/lisa-prntr.es.js"
}
